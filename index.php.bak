		<script type="text/javascript">
			function cycleImagesGadget() {
				var $active = $('.cycler.gadget .active');
				var $next = ($active.next().length > 0) ? $active.next() : $('.cycler.gadget img:first');
				$next.css('z-index', 2);
				//move the next image up the pile

				$active.fadeOut(1500, function() {//fade out the top image
					$active.css('z-index', 1).show().removeClass('active');
					//reset the z-index and unhide the image
					$next.css('z-index', 3).addClass('active');
					//make the next image the top one
				});
			}
			

			$(document).ready(function() {
				setInterval('cycleImagesGadget()', 3000);
				
			})
		</script>

		<script type="text/javascript">
			function cycleImagesBeauty() {
				var $active = $('.cycler.beauty .active');
				var $next = ($active.next().length > 0) ? $active.next() : $('.cycler.beauty img:first');
				$next.css('z-index', 2);
				//move the next image up the pile

				$active.fadeOut(1500, function() {//fade out the top image
					$active.css('z-index', 1).show().removeClass('active');
					//reset the z-index and unhide the image
					$next.css('z-index', 3).addClass('active');
					//make the next image the top one
				});
