		<script type="text/javascript">
			function checkWidth(init)
			{
				/*If browser resized, check width again */
				if ($(window).width() < 500) {
					$('#bg-xs').show();
					$('#bg-sm').hide();
					$('#bg-lg').hide();
					console.log('< 500')
				}else if ($(window).width() > 501 && $(window).width() < 991) {
					$('#bg-xs').hide();
					$('#bg-sm').show();
					$('#bg-lg').hide();
					console.log('> 501')
				}else if ($(window).width() > 992) {
					$('#bg-xs').hide();
					$('#bg-sm').hide();
					$('#bg-lg').show();
					console.log('993')
				}
			}

			$(document).ready(function() {
				checkWidth(true);

				$(window).resize(function() {
					checkWidth(false);
				});
			});
		</script>
